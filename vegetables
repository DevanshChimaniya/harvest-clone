function upsertSpCookie(test_id) {
	var test_bucket = "control";
	var idx = document.cookie.indexOf("sp-bucket=");
	var date = new Date();
	date.setTime(date.getTime() + (60 * 24 * 60 * 60 * 1000));
	var expires = "; expires=" + date.toUTCString();
	if (idx === -1) {
		// No cookie at all so set it.
		document.cookie = "sp-bucket=" + test_id + test_bucket + expires + "; path=/";
	} else {
		var idx_test_id = document.cookie.indexOf(test_id);
		if (idx_test_id === -1) {
			// Cookie exists, and lacks a cookie for this test.
			var idx_end = document.cookie.indexOf(";", idx);
			if (idx_end === -1) {
				var cv = document.cookie.substring(idx + 10);
			}else{
				var cv = document.cookie.substring(idx + 10, idx_end);
			}
			document.cookie = "sp-bucket=" + test_id + test_bucket + "|" + cv + expires + "; path=/";
		}
	}
}
upsertSpCookie("res-10355-");
upsertSpCookie("req-10355-");
